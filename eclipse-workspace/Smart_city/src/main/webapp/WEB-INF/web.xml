<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Login_pilgrim</servlet-name>
  	<display-name>Login_pilgrim</display-name>
  	<description></description>
  	<servlet-class>Pligrim_login_servlet.Login_pilgrim</servlet-class>
