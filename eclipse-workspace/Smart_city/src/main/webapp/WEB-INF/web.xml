  <servlet>
  	<servlet-name>RegisterPilgrim</servlet-name>
  	<display-name>RegisterPilgrim</display-name>
  	<description></description>
  	<servlet-class>pilgrim_register.RegisterPilgrim</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login_pilgrim</servlet-name>
  	<url-pattern>/Login_pilgrim</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterPilgrim</servlet-name>
  	<url-pattern>/RegisterPilgrim</url-pattern>
  </servlet-mapping>
</web-app>
