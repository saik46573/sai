public class PrimeArmstrong
{

	public static void main(String[] args)
	{
	Scanner sc=new Scanner(System.in);
	System.out.println("enter the number");
	  Prime p=new Prime();
	  p.calPrime(sc.nextInt());
	}

}
class Prime
{
	public int calPrime(int a)
	{
		return a;
	}
}
