package Day2;

public class string_ascending
{

	public static void main(String[] args) 
	{
		String s="AppPle";
		String s1="";
		int temp=0;
		for(int x=65;x<=122;x++)
		{
          for(int y=0;y<s.length();y++)
          {
        	 if((char)(x)==s.charAt(y))
        	 {
        		 s1=s1+s.charAt(y);
        	 }
          }
          
		}
		
		 System.out.println(s1);
	}

}
