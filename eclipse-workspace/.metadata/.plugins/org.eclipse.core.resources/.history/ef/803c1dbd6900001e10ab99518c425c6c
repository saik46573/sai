package exception1;

public class userdefinedExcepstion 
{
	public static void main(String[] args)
	{
		int num=8576;
		int x=0,rem=0,count=0,result=0;
		for( x=num;x>0;x/=10)
		{
			rem=x%10;
			break;
		}
		for(int y=num;y>0;y/=10)
		{
			count++;
		}
		for(int z=count;z>1;z--)
		{
			rem=rem*10;
		}
		System.out.println(x);
		System.out.println(rem);
	}

}
