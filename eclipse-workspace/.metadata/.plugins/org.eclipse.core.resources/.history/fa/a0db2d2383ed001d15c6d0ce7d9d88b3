package Day2;

public class string_ascending
{

	public static void main(String[] args) 
	{
		String s="ApPle";
		String s1="";
		int temp=0;
		for(int x=0;x<(s.length()-1);x++)
		{
          for(int y=x+1;x<s.length();y++)
          {
        	  int c=s.charAt(x);
        	  int d=s.charAt(y);
        	  if(c<d)
        	  {
        		 temp=y;
        		 y=x;
        		 x=temp;
        	  }
        	  else
        	  {
        		  temp=y;
         		 y=x;
         		 x=temp;
        	  }
        	  
          }
		}
		System.out.println(s1);

	}

}
